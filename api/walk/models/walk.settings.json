{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "walk",
  "info": {
    "name": "Walk"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "difficulty": {
      "type": "integer",
      "max": 5,
      "min": 0
    },
    "wheelchair_friendly": {
      "type": "boolean"
    },
    "kids_friendly": {
      "type": "boolean"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "checkpoint": {
      "type": "component",
      "repeatable": true,
      "component": "walk.checkpoint"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "Province": {
      "type": "enumeration",
      "enum": [
        "antwerpen",
        "limburg",
        "oost_vlaanderen",
        "west_vlaanderen",
        "vlaams_brabant",
        "waals_brabant",
        "henegouwen",
        "luik",
        "luxemburg",
        "namen"
      ],
      "required": true
    }
  }
}
