{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "Article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "published_at": {
      "type": "date",
      "required": true
    },
    "category": {
      "model": "category",
      "via": "articles"
    },
    "test": {
      "type": "component",
      "repeatable": true,
      "component": "test.test-comp"
    }
  }
}
